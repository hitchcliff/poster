{
  "name": "poster",
  "version": "1.0.0",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "watch": "tsc -w",
    "start": "nodemon dist/server.js",
    "dev": "npm-run-all -p watch start",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "cd src/migrations && typeorm migration:create",
    "migration:generate": "cd src/migrations && typeorm migration:create",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/data-source",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/data-source",
    "migration:sync": "npx typeorm-ts-node-commonjs schema:sync -d src/data-source.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.2.2",
    "@aws-sdk/client-s3": "^3.252.0",
    "apollo-server-express": "^3.11.1",
    "argon2": "^0.30.2",
    "body-parser": "^1.20.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.3",
    "graphql": "^15.3.0",
    "ioredis": "^5.2.4",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.19",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^15.0.2",
    "@types/node": "^18.11.9",
    "@types/nodemailer": "^6.4.6",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^9.0.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "tsconfig.json": "^1.0.11",
    "typescript": "^4.9.3"
  }
}
