{
  "name": "poster",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon dist/server.js",
    "start": "npm-run-all -p watch dev",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "cd src/migrations && typeorm migration:create",
    "migration:generate": "cd src/migrations && typeorm migration:create",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/data-source",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/data-source"
  },
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^3.11.1",
    "body-parser": "^1.20.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.3.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/graphql": "^14.5.0",
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "tsconfig.json": "^1.0.11",
    "typescript": "^4.9.3"
  }
}
